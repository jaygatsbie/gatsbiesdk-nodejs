{
  "name": "gatsbie",
  "version": "1.0.0",
  "description": "Official Node.js SDK for the Gatsbie Captcha API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "example": "tsx examples/main.ts"
  },
  "keywords": [
    "captcha",
    "datadome",
    "akamai",
    "turnstile",
    "recaptcha",
    "cloudflare",
    "perimeterx",
    "vercel"
  ],
  "author": "Gatsbie <support@gatsbie.io>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jaygatsbie/sdk-nodejs"
  },
  "homepage": "https://gatsbie.io",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
